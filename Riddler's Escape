            if (choice == "Anaconda")
            {
                favorite = new Anaconda();
                return favorite;
            }
            if (choice == "SeaTurtle")
            {
                favorite = new SeaTurtle();
                return favorite;
            }
            if (choice == "Chameleon")
            {
                favorite = new Chameleon();
                return favorite;
            }
            if (choice == "Rattlesnake")
            {
                favorite = new Rattlesnake();
                return favorite;
            }
            if (choice == "Frog")
            {
