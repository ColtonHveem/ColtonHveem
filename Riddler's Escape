        public static IDictionary<string, Animal> CreateZoo()
        {   
            Animal gorilla = new Gorilla();

            Animal cheetah = new Cheetah();

            Animal bluewhale = new BlueWhale();

            Animal zebra = new Zebra();

            Animal dolphin = new Dolphin();

            Animal alligator = new Alligator();

            Animal anaconda = new Anaconda();

            Animal seaturtle = new SeaTurtle();

            Animal chameleon = new Chameleon();

            Animal rattlesnake = new Rattlesnake();

            Animal frog = new Frog();

            Animal pelican = new Pelican();

            Animal hummingbird = new Hummingbird();

            Animal falcon = new Falcon();

            Animal penguin = new Penguin();

            Animal ostrich = new Ostrich();

            Animal greatwhiteshark = new GreatWhiteShark();

            Animal clownfish = new Clownfish();

            Animal stingray = new Stingray();

            Animal seahorse = new Seahorse();

            Animal octopus = new Octopus();

            Animal cuttlefish = new Cuttlefish();

            Animal blackwidowspider = new BlackWidowSpider();

            Animal starfish = new Starfish();

            Animal snail = new Snail();

            //Animal[] Zoo = { gorilla, cheetah } ;
            //return Zoo;

            IDictionary<string, Animal> Zoo = new Dictionary<string, Animal>() { { "Gorilla", gorilla }, { "Cheetah", cheetah }, { "BlueWhale", bluewhale }, { "Zebra", zebra }, { "Dolphin", dolphin }, { "Alligator", alligator }, { "Anaconda", anaconda }, { "SeaTurtle", seaturtle },{ "Chameleon", chameleon }, { "Rattlesnake", rattlesnake }, { "Frog", frog }, { "Pelican", pelican }, { "Hummingbird", hummingbird }, { "Falcon", falcon }, { "Penguin", penguin }, { "Ostrich", ostrich }, { "GreatWhiteShark", greatwhiteshark }, { "Clownfish", clownfish }, { "Stingray", stingray }, { "Seahorse", seahorse }, { "Octopus", octopus }, { "Cuttlefish", cuttlefish }, { "BlackWidowSpider", blackwidowspider }, { "Starfish", starfish }, { "Snail", snail } };
            return Zoo;
            //Zoo.Add("Gorilla", gorilla);
        }
