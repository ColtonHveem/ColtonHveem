    public class Program
    {
        enum AnimalType
        {
            gorilla, cheetah, bluewhale, zebra, dolphin, alligator, anaconda, seaturtle, chameleon, rattlesnake, frog, pelican, hummingbird, falcon, penguin, ostrich, greatwhiteshark, clownfish, stingray, seahorse, octopus, cuttlefish, blackwidowspider, starfish, snail
            //if (Zoo[(int) AnimalType.gorilla].Speed > Zoo[(int) AnimalType.cheetah].Speed)
        }
        public static void Main(string[] args)
        {
            Title();
            NameandIntro();
            HotelRiddle();
            LogicRiddle();
            DarwinRiddleIntro();
            DarwinRiddleNotes();
            DarwinRiddle();
            Outro();

        }
        public static void Title()
        {
            int milliseconds2 = 4000;

            Console.WriteLine(@"
