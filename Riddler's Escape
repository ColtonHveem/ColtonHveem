        public static void DarwinRiddle()
        {
            int milliseconds = 2000;
            int milliseconds2 = 4000;

            Animal favorite = GetAnimal();
            double favMaxSize = (favorite.Size * 1.1);
            double favMinSize = (favorite.Size * .9);
            double favMaxSpeed = (favorite.Speed * 1.1);
            double favMinSpeed = (favorite.Speed * .9);

            IDictionary<string, Animal> Zoo = CreateZoo();

            Console.Write("Enter Question: ");
            string question = Console.ReadLine();
            question = question.ToLower();
            
            //if (Zoo["Gorilla"].Size > Zoo["Cheetah"].Speed)
            //{
            //    Console.WriteLine("Yes");
            //}
            //else
            //{
            //    Console.WriteLine("No");
            //}

            int count = 0;

            while (count < 5)
            {
                if (question == "is it a mammal?" || question == "mammal?")
                {
                    if (favorite.Parent == "mammal")
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it a reptile?" || question == "reptile?")
                {
                    if (favorite.Parent == "reptile")
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it an amphibian?" || question == "amphibian?")
                {
                    if (favorite.Parent == "amphibian")
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it a bird?" || question == "bird?")
                {
                    if (favorite.Parent == "bird")
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it a fish?" || question == "fish?")
                {
                    if (favorite.Parent == "fish")
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it an invertebrate?" || question == "invertebrate?")
                {
                    if (favorite.Parent == "invertebrate")
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it larger than a gorilla?" || question == "larger than gorilla?")
                {
                    if (favMinSize > Zoo["Gorilla"].Size)
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else if (favMaxSize < Zoo["Gorilla"].Size)
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("It's about the same size");
                        Console.WriteLine();
                    }
                }
                else if (question == "is it larger than a cheetah?" || question == "larger than cheetah?")
                {
                    if (favMinSize > Zoo["Cheetah"].Size)
                    {
                        Console.WriteLine("Answer: Yes");
                        Console.WriteLine();
                    }
                    else if (favMaxSize < Zoo["Cheetah"].Size)
                    {
                        Console.WriteLine("Answer: No");
                        Console.WriteLine();
                    }
                    else
                    {
                        Console.WriteLine("It's about the same size");
                        Console.WriteLine();
